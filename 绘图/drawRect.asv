function drawT(x,y,r)
a=ones(1,2);
a(1,1)=x;
a(1,2)=y;
k=2;
for i=-r/2:r:r/2
    for j=-r/2:r:r/2
        a(k,1)= x+i;
        a(k,2)=y+j;
        k=k+1;
    end
end
scatter(a,b,16,'filled','k');
end

